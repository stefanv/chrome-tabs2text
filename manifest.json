{
  "manifest_version": 3,
  "name": "tabs2text",
  "description": "Export list of open tabs as markdown / org text",
  "version": "0.1",
  "permissions": ["tabs", "tabGroups", "downloads"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
